{
	"name": "t7/http-request",
	"license": "MIT",
	"require": {
		"php": ">=8.3"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^3.64",
		"josephscott/phpcsfixer-config": "^0.0.4",
		"phpstan/phpstan": "^1.12",
		"pestphp/pest": "^3.5.0"
	},
	"autoload": {
		"classmap": [
			"src/t7/http/request.php",
			"src/t7/http/response.php"
		]
	},
	"config": {
		"classmap-authoritative": true,
		"optimize-autoloader": true,
		"allow-plugins": false
	}
}
